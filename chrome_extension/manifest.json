{
  "name": "vtools",
  "description": "vtuber tools",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "host_permissions": [
    "http://*/",
    "https://*/"
  ]
}
